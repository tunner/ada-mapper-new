{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Generate Mappers",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tools/gen_mapper.py",
      "args": [
        "${workspaceFolder}/mappings.json",
        "${workspaceFolder}/src"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {}
    },
    {
      "name": "Python: Generate + Validate",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tools/gen_mapper.py",
      "args": [
        "${workspaceFolder}/mappings.json",
        "${workspaceFolder}/src",
        "--validate"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
